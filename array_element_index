#include <stdio.h>

int main()
{
    int a[1000],n,num,flag=0,i;
    scanf("%d",&num);
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
        if(a[i]==num) 
        {
            printf("%d",i); 
            flag=1;
            break;
        }
    }
    if(flag==0)
    {
       printf("-1");
    }
}


